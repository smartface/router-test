{
    ".label": {
        "width": 250,
        "height": 70,
        "touchEnabled": true,
        "visible": true,
        "multiline": false,
        "backgroundColor": "rgba(0,0,0,0)",
        "alpha": 1,
        "borderColor": "rgba(0,0,0,1)",
        "borderWidth": 0,
        "textColor": "#000000",
        "textAlignment": "MIDLEFT",
        "font": {
            "size": 16,
            "bold": false,
            "italic": false,
            "family": "Default"
        },
        "flexProps": {
            "alignSelf": "AUTO",
            "positionType": "RELATIVE"
        },
        "&-small": {
            "width": 100
        },
        "&-medium": {
            "width": 250
        },
        "&-large": {
            "width": 350
        }
    }
}

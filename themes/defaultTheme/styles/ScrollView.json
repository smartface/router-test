{
	".scrollView": {
		"height": 200,
		"width": 200,
		"touchEnabled": true,
		"visible": true,
		"backgroundColor": "#FFFFFF",
		"alpha": 1,
		"borderColor": "rgba(0,0,0,1)",
		"borderWidth": 0,
		"scrollBarEnabled": true,
		"align": "VERTICAL",
		"flexProps": {
			"alignSelf": "AUTO",
			"direction": "INHERIT",
			"alignContent": "STRETCH",
			"alignItems": "STRETCH",
			"justifyContent": "FLEX_START",
			"flexWrap": "NOWRAP",
			"flexDirection": "COLUMN",
			"positionType": "RELATIVE"
		},
		"layout": {
			"flexProps": {
				"direction": "INHERIT",
				"alignContent": "STRETCH",
				"alignItems": "STRETCH",
				"justifyContent": "FLEX_START",
				"flexWrap": "NOWRAP",
				"flexDirection": "COLUMN"
			},
			"visible": true,
			"backgroundColor": "rgba(1, 1, 1, 0)",
			"alpha": 1,
			"borderColor": "rgba(0,0,0,1)",
			"borderWidth": 0
		},
		"&-small": {
			"width": 100
		},
		"&-medium": {
			"width": 250
		},
		"&-large": {
			"width": 350
		}
	}
}

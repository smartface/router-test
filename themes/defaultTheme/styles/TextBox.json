{
	".textBox": {
		"height": 70,
		"width": 200,
		"touchEnabled": true,
		"visible": true,
		"backgroundColor": "#FFFFFF",
		"alpha": 1,
		"borderColor": "rgba(0,0,0,1)",
		"borderWidth": 0,
		"textColor": "rgba(0,0,0,1)",
		"textAlignment": "MIDLEFT",
		"font": {
			"size": 16,
			"bold": false,
			"italic": false,
			"family": "Default"
		},
		"flexProps": {
			"alignSelf": "AUTO",
			"positionType": "RELATIVE"
		},
		"&-small": {
			"width": 100
		},
		"&-medium": {
			"width": 250
		},
		"&-large": {
			"width": 350
		}
	},
	".sf-textBox": {
		"height": 54,
		"width": null,
		"borderColor": "#00a1f1",
		"borderWidth": 1,
		"borderRadius": 4,
		"textColor": "#000000",
		"textAlignment": "MIDLEFT",
		"marginTop": 1,
		"marginBottom": 1,
		"font": {
			"size": 16,
			"bold": false,
			"italic": false,
			"family": "SFProText",
			"style": "Regular"
		},
		"flexProps": {
			"alignSelf": "AUTO",
			"positionType": "RELATIVE"
		},
		"&-small": {
			"width": 100
		},
		"&-medium": {
			"width": 250
		},
		"&-large": {
			"width": 350
		}
	},
	".default_textBox": {
		"textAlignment": "MIDLEFT"
	}
}
{
	".activityIndicator": {
		"touchEnabled": true,
		"width": 42,
		"height": 42,
		"color": "#ADADAD",
		"visible": true,
		"backgroundColor": "#FFFFFF",
		"alpha": 1,
		"borderColor": "rgba(0,0,0,1)",
		"borderWidth": 0,
		"flexProps": {
			"positionType": "RELATIVE",
			"alignSelf": "AUTO"
		},
		"ios": {
			"activityIndicatorViewStyle": "NORMAL"
		},
		"minHeight": 21,
		"minWidth": 21,
		"&-small": {
			"width": 100
		},
		"&-medium": {
			"width": 250
		},
		"&-large": {
			"width": 350
		}
	},
	".sf-activityIndicator": {
		"width": 42,
		"height": 42,
		"color": "#00a1f1",
		"flexProps": {
			"positionType": "RELATIVE",
			"alignSelf": "CENTER"
		},
		"ios": {
			"activityIndicatorViewStyle": "LARGE"
		},
		"marginTop": 1,
		"marginBottom": 1,
		"minHeight": 21,
		"minWidth": 21,
		"&-small": {
			"width": 100
		},
		"&-medium": {
			"width": 250
		},
		"&-large": {
			"width": 350
		}
	}
}